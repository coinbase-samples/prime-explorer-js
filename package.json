{
  "name": "prime-explorer-js",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "watch:tailwind": "postcss tailwind.css -o index.css -w",
    "build:tailwind": "cross-env NODE_ENV=production postcss tailwind.css -o index.css",
    "build": "npm run build:tailwind && rollup -c",
    "start": "node server.js",
    "serve": "serve public -p 80",
    "dev": "concurrently \"rollup -c -w\" \"npm run watch:tailwind\"",
    "format": "prettier --write '{public,src}/**/*.{css,html,js,svelte}'",
    "lint": "eslint --fix --quiet src --ext .js,.svelte"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "22.0.1",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "13.3.0",
    "@rollup/plugin-replace": "^3.0.0",
    "autoprefixer": "10.4.7",
    "eslint": "^8.19.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "postcss": "^8.3.7",
    "postcss-cli": "^10.0.0",
    "prettier": "^2.7.1",
    "prettier-plugin-organize-imports": "^3.0.0",
    "prettier-plugin-tailwindcss": "^0.1.12",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-license": "^2.8.1",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "svelte": "^3.49.0",
    "svelte-forms-lib": "^2.0.1",
    "svelte-routing": "^1.6.0",
    "tailwindcss": "^2.2.15"
  },
  "dependencies": {
    "carbon-components-svelte": "^0.41.0",
    "carbon-preprocess-svelte": "^0.6.0",
    "coinbase-pro-node": "^3.0.8",
    "concurrently": "^6.2.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "crypto-es": "^1.2.7",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-http-proxy": "^1.6.2",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "npm": "^8.14.0",
    "path": "^0.12.7",
    "rollup-plugin-npm": "^2.0.0",
    "uuid": "^8.3.2"
  }
}
