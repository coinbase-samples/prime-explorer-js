{
  "name": "svelte-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "watch:tailwind": "postcss tailwind.css -o index.css -w",
    "build:tailwind": "cross-env NODE_ENV=production postcss tailwind.css -o index.css",
    "build": "npm run build:tailwind && rollup -c",
    "start": "node server.js",
    "serve": "serve public -p 80",
    "dev": "concurrently \"rollup -c -w\" \"npm run watch:tailwind\"",
    "format": "prettier --write '{public,src}/**/*.{css,html,js,svelte}'",
    "lint": "eslint --fix --quiet src --ext .js,.svelte"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "22.0.1",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "13.3.0",
    "@rollup/plugin-replace": "^3.0.0",
    "@snowpack/plugin-webpack": "^3.0.0",
    "autoprefixer": "10.4.7",
    "eslint": "^8.19.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "postcss": "^8.3.7",
    "prettier": "^2.7.1",
    "prettier-plugin-organize-imports": "^3.0.0",
    "prettier-plugin-tailwindcss": "^0.1.12",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-license": "^2.8.1",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "src": "^1.0.0",
    "svelte": "^3.0.0",
    "tailwindcss": "^2.2.15"
  },
  "dependencies": {
    "api": "^5.0.0-beta.1",
    "async": "^3.2.2",
    "axios": "^0.21.4",
    "buffer": "^6.0.3",
    "carbon-components-svelte": "^0.41.0",
    "carbon-preprocess-svelte": "^0.6.0",
    "clean": "^4.0.2",
    "coinbase-pro-node": "^3.0.8",
    "commander": "^8.3.0",
    "concurrently": "^6.2.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "crypto-es": "^1.2.7",
    "crypto-js": "^4.1.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-http-proxy": "^1.6.2",
    "fixparser": "^5.2.1",
    "http": "0.0.1-security",
    "http-proxy-middleware": "^2.0.1",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "npm": "^8.14.0",
    "path": "^0.12.7",
    "postcss-cli": "^8.3.1",
    "query-string": "^7.0.1",
    "resolve": "^1.20.0",
    "rollup-plugin-npm": "^2.0.0",
    "sirv-cli": "^1.0.0",
    "svelte-chartjs": "^1.1.3",
    "svelte-forms-lib": "^1.10.7",
    "svelte-router-spa": "^6.0.2",
    "svelte-routing": "^1.6.0",
    "svelte-websocket-store": "^1.1.15",
    "to-uuid": "^0.1.3",
    "url-parse": "^1.5.3",
    "uuid": "^8.3.2",
    "uuidv4": "^6.2.13"
  }
}
